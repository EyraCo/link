{
  "**/email/*.html.eex": {
    "type": "html-email",
    "alternate": "{dirname}/email/{basename}.text.eex"
  },
  "**/email/*.text.eex": {
    "type": "text-email",
    "alternate": "{dirname}/email/{basename}.html.eex"
  },
  "test/*_test.exs": {
    "type": "test",
    "alternate": "lib/{}.ex"
  },
  "lib/*.ex": {
    "type": "code",
    "alternate": "test/{}_test.exs"
  },
  "lib/*.html.eex": {
    "type": "template"
  },
  "priv/**/migrations/*.exs": {
    "type": "migration"
  },
  "guides/*.md": {
    "type": "guide"
  },
  "config/*.exs": {
    "type": "config"
  }
}
