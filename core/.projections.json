{
  "**/email/*.html.eex": {
    "type": "html-email",
    "alternate": "{dirname}/email/{basename}.text.eex"
  },
  "**/email/*.text.eex": {
    "type": "text-email",
    "alternate": "{dirname}/email/{basename}.html.eex"
  },
  "test/*_test.exs": {
    "type": "test",
    "alternate": "lib/{}.ex",
    "template": [
      "defmodule {camelcase|capitalize|dot}Test do",
      "  use Core.DataCase, async: true",
      "  alias Core.Factories",
      "  alias {camelcase|capitalize|dot}",
      "",
      "  describe \"\" do",
      "    test \"\" do",
      "    end",
      "  end",
      "end"
    ]
  },
  "lib/*.ex": {
    "type": "code",
    "alternate": "test/{}_test.exs"
  },
  "lib/*.html.eex": {
    "type": "template"
  },
  "priv/**/migrations/*.exs": {
    "type": "migration"
  },
  "guides/*.md": {
    "type": "guide"
  },
  "config/*.exs": {
    "type": "config"
  }
}
