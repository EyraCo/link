<ul>
  <%= if @current_user do %>
    <li><%= @current_user.email %></li>
    <li><%= link "Settings", to: ~p"/user/settings" %></li>
    <li>
      <%= link "Log out", to: ~p"/user/session" %>
    </li>
  <% else %>
    <li>
      <%= link "Register", to: ~p"/user/signup" %>
    </li>
    <li><%= link "Log in", to: ~p"/user/signin" %></li>
  <% end %>
</ul>
